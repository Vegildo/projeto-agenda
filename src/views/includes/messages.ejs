<% if (errors.length > 0) { %>
<div class="row">
    <div class="col my-3">
        <div class="alert alert-danger text-center text-danger">
            <% errors.forEach(erro => { %>
                <%= erro %><br>
            <% }); %>
        </div>
    </div>
</div>
<% } %>
<% if (success.length > 0) { %>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success text-center text-success">
                <% success.forEach(el => { %>
                    <%= el %><br>
                <% }); %>
            </div>
        </div>
    </div>
    <% } %>